<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vista_alarmas">


    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="278dp"
        android:layout_height="33dp"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/alarma_mi_ciclo__2_" />


    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="56dp"
        android:layout_marginBottom="674dp"
        android:background="#1A000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/crear_alarma"
        android:layout_width="47dp"
        android:layout_height="43dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:drawableBottom="@drawable/ic_baseline_add_circle_24"
        android:textAlignment="textStart"
        app:backgroundTint="#ffffff"
        app:layout_constraintStart_toEndOf="@+id/alarmar_activa"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/alarmar_activa"
        android:layout_width="45dp"
        android:layout_height="41dp"
        android:layout_marginStart="216dp"
        android:layout_marginTop="36dp"
        android:drawableBottom="@drawable/icon"
        android:textAlignment="textStart"
        app:backgroundTint="#ffffff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:strokeColor="#000000" />

    <Button
        android:id="@+id/configuracion"
        android:layout_width="45dp"
        android:layout_height="42dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="36dp"
        android:drawableBottom="@drawable/group_19"
        android:textAlignment="textStart"
        app:backgroundTint="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/crear_alarma"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:strokeColor="#000000" />

    <View
        android:id="@+id/myRectangleView"
        android:layout_width="377dp"
        android:layout_height="59dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/icon_rectangel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/alarmar_activa" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/nunito"
        android:text="9:15 AM"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nunito"
        android:text="Menstruación"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="67dp"
        android:layout_height="36dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:checked="true"
        android:fontFamily="@font/nunito"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumb="@drawable/bg_thumb"
        android:track="@drawable/bg_track"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView"
        app:layout_constraintHorizontal_bias="0.193"
        app:layout_constraintStart_toEndOf="@+id/textView24"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView" />

    <View
        android:id="@+id/myRectangleView1"
        android:layout_width="377dp"
        android:layout_height="59dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/icon_rectangel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView" />


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/nunito"
        android:text="1:15 PM"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView1"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nunito"
        android:text="Ovulación"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView1" />

    <Switch
        android:id="@+id/switch2"
        android:layout_width="67dp"
        android:layout_height="36dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="12dp"
        android:checked="false"
        android:fontFamily="@font/nunito"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumb="@drawable/bg_thumb"
        android:track="@drawable/bg_track"
        app:layout_constraintEnd_toEndOf="@+id/myRectangleView1"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView1" />


    <View
        android:id="@+id/myRectangleView2"
        android:layout_width="377dp"
        android:layout_height="59dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/icon_rectangel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView1" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/nunito"
        android:text="2:15 PM"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView2"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nunito"
        android:text="Días infértil"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView2" />

    <Switch
        android:id="@+id/switch3"
        android:layout_width="67dp"
        android:layout_height="36dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="12dp"
        android:checked="true"
        android:fontFamily="@font/nunito"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumb="@drawable/bg_thumb"
        android:track="@drawable/bg_track"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView2" />

    <View
        android:id="@+id/myRectangleView3"
        android:layout_width="377dp"
        android:layout_height="59dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/icon_rectangel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView2" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/nunito"
        android:text="3:15 PM"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/myRectangleView3"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView3" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nunito"
        android:text="Días  fértil"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView3" />

    <Switch
        android:id="@+id/switch4"
        android:layout_width="67dp"
        android:layout_height="36dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="12dp"
        android:checked="true"
        android:fontFamily="@font/nunito"
        android:scaleX="1.2"
        android:scaleY="1.2"
        android:thumb="@drawable/bg_thumb"
        android:track="@drawable/bg_track"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/myRectangleView3" />

    <Button
        android:id="@+id/inicio"
        android:layout_width="112dp"
        android:layout_height="40dp"
        android:layout_marginTop="144dp"
        android:background="@drawable/rounded_corner"
        android:fontFamily="@font/nunito"
        android:hapticFeedbackEnabled="false"
        android:text="Inicio"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:backgroundTint="#e4ccda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myRectangleView3"
        app:layout_constraintVertical_bias="0.096" />


</androidx.constraintlayout.widget.ConstraintLayout>